<html lang="en">
	<head>
		<%- include('./partials/head') %>
		<link rel="stylesheet" href="css/register.css">
	</head>
	<body class="body_registro">
		<!-- header -->
		<%- include('./partials/header') %>
		<main>
			<!-- <section> -->
				<form class="formulario">
					<legend>Formulario de Registro</legend>
					 <section class="contenedor-campos">
					
							<fieldset class="campo">
								<label for="nombre">Nombre y Apellido</label>
								<input
									class="input-text"
									type="text"
									id="nombre"
									placeholder="Nombre Completo"
								/>
							</fieldset>
							<fieldset class="campo">
								<label for="dni">DNI</label>
								<input
									class="input-text"
									type="number"
									id="dni"
									placeholder="DNI"
								/>
							</fieldset>
							<fieldset class="campo">
								<label for="email">Correo</label>
								<input
									class="input-text"
									type="email"
									id="email"
									placeholder="Email"
								/>
							</fieldset>
							<fieldset class="campo">
								<label for="pass1">Contrase単a</label>
								<input
									class="input-text"
									type="password"
									id="pass1"
									placeholder="Contrase単a"
								/>
							</fieldset>
							<fieldset class="campo">
								<label for="birth">Fecha de Nacimiento</label>
								<input class="input-text" id="birth" type="date" />
							</fieldset>
							<fieldset class="campo">
								<label for="pass2">Confirmar Contrase単a</label>
								<input
									class="input-text"
									type="password"
									id="pass2"
									placeholder="Confirmar Contrase単a"
								/>
							</fieldset>
					    </section>
						<!-- .contenedor-campos -->
						<input class="boton w-sm-100 alinear-centro flex" type="submit" value="Registrarse" />
					
				</form>
		</main>
	</body>
</html>
