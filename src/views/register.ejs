<html lang="en">
	<head>
		<%- include('./partials/head') %>
	</head>
	<body class="body_registro">
		<!-- header -->
		<%- include('./partials/header') %>
		<main>
			<section>
				<form class="formulario">
					<fieldset>
						<legend>Formulario de Registro</legend>
						<div class="contenedor-campos">
							<div class="campo">
								<label for="nombre">Nombre y Apellido</label>
								<input
									class="input-text"
									type="text"
									id="nombre"
									placeholder="Nombre Completo"
								/>
							</div>
							<div class="campo">
								<label for="dni">DNI</label>
								<input
									class="input-text"
									type="number"
									id="dni"
									placeholder="DNI"
								/>
							</div>
							<div class="campo">
								<label for="email">Correo</label>
								<input
									class="input-text"
									type="email"
									id="email"
									placeholder="Email"
								/>
							</div>
							<div class="campo">
								<label for="pass1">Contrase単a</label>
								<input
									class="input-text"
									type="password"
									id="pass1"
									placeholder="Contrase単a"
								/>
							</div>
							<div class="campo">
								<label for="birth">Fecha de Nacimiento</label>
								<input class="input-text" id="birth" type="date" />
							</div>
							<div class="campo">
								<label for="pass2">Confirmar Contrase単a</label>
								<input
									class="input-text"
									type="password"
									id="pass2"
									placeholder="Confirmar Contrase単a"
								/>
							</div>
						</div>
						<!-- .contenedor-campos -->
						<div class="alinear-derecha flex">
							<input class="boton w-sm-100" type="submit" value="Registrarse" />
						</div>
					</fieldset>
				</form>
			</section>
		</main>
	</body>
</html>
